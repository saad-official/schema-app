{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "status": { "type": "string" },
      "status_code": { "type": "number" },
      "message": { "type": "string" },
      "data": {
        "type": "object",
        "properties": {
          "userId": { "type": "number" },
          "email": { "type": "string" },
          "firstName": { "type": "string" },
          "lastName": { "type": ["string", "null"] },
          "gmail_integration": {
            "type": "array",
            "items": { "$ref": "#/definitions/integration" }
          },
          "outlook_integration": {
            "type": "array",
            "items": { "$ref": "#/definitions/integration" }
          },
          "zoom_integration": {
            "type": "array",
            "items": { "$ref": "#/definitions/integration" }
          },
          "contract_type": { "type": "string" }
        },
        "required": ["userId", "email", "contract_type", "firstName", "lastName"]
      }
    },
    "required": ["status", "status_code", "message", "data"],
    "definitions": {
      "integration": {
        "type": "object",
        "properties": {
          "platform_id": { "type": "string" },
          "email": { "type": "string" },
          "is_active": { "type": "boolean" }
        },
        "required": ["platform_id", "email", "is_active"]
      }
    }
  }
  